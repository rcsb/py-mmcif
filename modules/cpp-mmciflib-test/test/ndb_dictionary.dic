    _entity.id
    _entity.type
    _entity.formula_weight
    _entity.details
      1  polymer       10916
    ;                  The enzymatically competent form of HIV protease is a
                       dimer. This entity corresponds to one monomer of an
                       active dimer.
    ;
      2  non-polymer  '762'  '.'
      3  water         18  '.'

;
# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
     save_

